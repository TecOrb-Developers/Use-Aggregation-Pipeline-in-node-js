{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@models/*": [
        "src/models/*"
      ],
      "@server": [
        "src/server"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@controllers/*": [
        "src/controllers/*"
      ]
    },
    "useUnknownInCatchVariables": false
  },
  "include": [
    "src/**/*.ts",
    "spec/**/*.ts",
    "src/public/web/scripts/dashboard.js",
    "src/routes/app/door_Keeper",
    "src/public/admin/scripts/listing/vendor_category.JS"
  ],
  "exclude": [
    "src/public/"
  ]
}